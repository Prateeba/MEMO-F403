	year = {2002},
	doi = {10.1007/3-540-45465-9_35},
	pages = {401--413},
	file = {Hearn and Demaine - 2002 - The Nondeterministic Constraint Logic Model of Com.pdf:/home/prat/Zotero/storage/5AFD6EIG/Hearn and Demaine - 2002 - The Nondeterministic Constraint Logic Model of Com.pdf:application/pdf}
