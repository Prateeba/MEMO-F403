	number = {12-14},
	urldate = {2019-05-24},
	journal = {Theoretical Computer Science},
	author = {Ito, Takehiro and Demaine, Erik D. and Harvey, Nicholas J.A. and Papadimitriou, Christos H. and Sideri, Martha and Uehara, Ryuhei and Uno, Yushi},
